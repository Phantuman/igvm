name="{{ hostname }}"
kernel="/boot/vmlinuz-2.6-xenU"
root="/dev/xvda1 ro"
memory={{ mem }}
maxmem=16384
vcpus={{ num_cpu }}
maxvcpus=24
disk=['phy:{{ device }},xvda1,w']
vif=['']
extra="2 init=/sbin/init console=hvc0 rootflags=allocsize=64k mem={{ max_mem }}M elevator=noop"
ramdisk="/boot/initrd.img-3.6.6-xenu-ig7"
